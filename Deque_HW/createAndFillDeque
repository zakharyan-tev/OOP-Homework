#include <iostream>
#include <deque>

void createAndFillDeque(int N) {
    std::deque<int> dq;

    for (int i = 1; i <= N; ++i) {
        if (i % 2 == 0)
            dq.push_front(i);
        else
            dq.push_back(i);
    }

    for (int x : dq)
        std::cout << x << " ";
    std::cout << std::endl;
}

int main() {
    createAndFillDeque(10);
    return 0;
}
