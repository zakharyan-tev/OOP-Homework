#include <iostream>
#include <queue>
#include <string>

class TaskProcessor {
private:
    std::queue<std::string> tasks;

public:
    void addTask(const std::string& task) {
        tasks.push(task);
    }

    void processNext() {
        if (!tasks.empty()) {
            std::cout << "Processing: " << tasks.front() << std::endl;
            tasks.pop();
        } else {
            std::cout << "No tasks to process." << std::endl;
        }
    }

    bool isEmpty() const {
        return tasks.empty();
    }
};

int main() {
    TaskProcessor tp;
    tp.addTask("Compile");
    tp.addTask("Link");
    tp.addTask("Run");

    while (!tp.isEmpty())
        tp.processNext();

    return 0;
}
