#include <iostream>
#include <array>

template <typename T, std::size_t N>
bool validateArray(const std::array<T, N>& arr, T min, T max) {
    for (const auto& elem : arr) {
        if (elem < min || elem > max) {
            return false;
        }
    }
    return true;
}

int main() {
    std::array<int, 4> scores = {85, 92, 78, 96};
    bool valid = validateArray<int, 4>(scores, 0, 100);

    std::cout << std::boolalpha << valid << std::endl;

    std::array<int, 4> wrongScores = {85, -5, 78, 110};
    bool valid2 = validateArray<int, 4>(wrongScores, 0, 100);

    std::cout << std::boolalpha << valid2 << std::endl;
}
