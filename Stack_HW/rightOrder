#include <iostream>
#include <stack>

using namespace std;

bool is_balanced(const string& s) {
    stack<char> stack;
    
    for (char c : s) {
        if (c == '(' || c == '{' || c == '[') {
            stack.push(c);
        } else if (c == ')' || c == '}' || c == ']') {
            if (stack.empty()) {
                return false;
            }
            
            char top = stack.top();
            if ((c == ')' && top != '(') || 
                (c == '}' && top != '{') || 
                (c == ']' && top != '[')) {
                return false;
            }
            
            stack.pop();
        }
    }
    
    return stack.empty();
}

int main() {
    string input1 = "{ [ ( ) ( ) ] }";
    string input2 = "( [ ) ]";
    
    if (is_balanced(input1)) {
        cout << "YES" << endl;
    } else {
        cout << "NO" << endl;
    }
    
    if (is_balanced(input2)) {
        cout << "YES" << endl;
    } else {
        cout << "NO" << endl;
    }

    return 0;
}
